{
    "compilerOptions": {
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "outDir": "./built/",
        "allowJs": false,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "strict": true,
        "sourceMap": true
  },
  "include": [
    "./src/**/*"
  ]
}
